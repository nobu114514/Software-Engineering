{"ast":null,"code":"export default {\n  name: 'CustomerOrders',\n  data() {\n    return {\n      orders: [\n      // 测试数据，当API调用失败时也能看到订单列表的显示效果\n      {\n        id: 1,\n        address: '北京市朝阳区某某街道100号',\n        created_at: new Date().toISOString(),\n        is_completed: true,\n        buyer_name: '张三',\n        notes: '请尽快送达',\n        phone: '13800138000',\n        product_name: '测试商品1',\n        price: 99.99,\n        status: 'completed'\n      }, {\n        id: 2,\n        address: '上海市浦东新区某某路200号',\n        created_at: new Date().toISOString(),\n        is_completed: false,\n        buyer_name: '李四',\n        notes: '',\n        phone: '13900139000',\n        product_name: '测试商品2',\n        price: 199.99,\n        status: 'pending'\n      }],\n      loading: false,\n      error: '',\n      statusFilter: '',\n      timeFilter: 'desc',\n      showDetail: false,\n      selectedOrder: null,\n      debugInfo: '' // 用于显示调试信息\n    };\n  },\n  created() {\n    console.log('CustomerOrders组件已创建，准备获取订单数据');\n    // 立即调用fetchOrders方法\n    this.fetchOrders();\n\n    // 为了测试，再延迟3秒后再次调用\n    setTimeout(() => {\n      console.log('3秒后再次尝试获取订单数据...');\n      this.fetchOrders();\n    }, 3000);\n  },\n  methods: {\n    async fetchOrders() {\n      this.loading = true;\n      this.error = '';\n      this.debugInfo = '开始获取订单数据...\\n';\n      try {\n        // 从localStorage获取当前登录用户名\n        const customerUsername = localStorage.getItem('customerUsername');\n        this.debugInfo += `localStorage内容: ${JSON.stringify(localStorage, null, 2)}\\n`;\n        this.debugInfo += `获取的customerUsername: ${customerUsername}\\n`;\n\n        // 验证用户是否已登录\n        if (!customerUsername) {\n          this.error = '请先登录再查看订单';\n          this.debugInfo += '未找到登录用户信息\\n';\n          // 保留测试数据，不清除\n          return;\n        }\n        this.debugInfo += `当前登录用户: ${customerUsername}\\n`;\n\n        // 构建请求参数\n        const params = {\n          status: this.statusFilter || undefined,\n          sort: this.timeFilter\n        };\n\n        // 构建请求头\n        const headers = {\n          'X-Username': customerUsername\n        };\n        this.debugInfo += `请求参数: ${JSON.stringify(params, null, 2)}\\n`;\n        this.debugInfo += `请求头: ${JSON.stringify(headers, null, 2)}\\n`;\n        this.debugInfo += `axios基础URL: ${this.$axios.defaults.baseURL || '未设置'}\\n`;\n        // 修复重复API前缀问题 - 基础URL已经包含/api，所以只需要/orders\n        this.debugInfo += `完整请求URL: ${this.$axios.defaults.baseURL ? this.$axios.defaults.baseURL + '/orders' : '/api/orders'}\\n`;\n        this.debugInfo += '开始发送API请求...\\n';\n\n        // 发送API请求，带上筛选参数和用户信息 - 修复API路径和中文编码问题\n        // 修复重复API前缀：基础URL已经包含/api，所以只需要/orders\n        // 修复中文编码问题：不使用X-Username请求头，改用params传递用户名\n        const apiParams = {\n          ...params,\n          username: customerUsername\n        }; // 将用户名放在query参数中\n        this.debugInfo += `最终请求参数: ${JSON.stringify(apiParams, null, 2)}\\n`;\n        const response = await this.$axios.get(`/orders`, {\n          params: apiParams\n          // 移除包含中文的请求头\n        });\n        this.debugInfo += 'API请求成功!\\n';\n        this.debugInfo += `响应状态: ${response.status}\\n`;\n        this.debugInfo += `响应状态文本: ${response.statusText}\\n`;\n        this.debugInfo += `API响应数据: ${JSON.stringify(response.data, null, 2)}\\n`;\n\n        // 只有API调用成功时才更新订单数据\n        if (response.data && Array.isArray(response.data)) {\n          this.orders = response.data;\n          this.debugInfo += `已更新订单数据，共 ${response.data.length} 条订单\\n`;\n        } else {\n          this.debugInfo += 'API返回的数据格式不正确，使用测试数据\\n';\n          this.debugInfo += `响应数据类型: ${typeof response.data}\\n`;\n          this.debugInfo += `是否为数组: ${Array.isArray(response.data)}\\n`;\n        }\n      } catch (err) {\n        this.error = '获取订单列表失败，显示测试数据';\n        this.debugInfo += `API请求错误: ${err.message || '未知错误'}\\n`;\n        if (err.response) {\n          this.debugInfo += `错误响应状态: ${err.response.status}\\n`;\n          this.debugInfo += `错误响应状态文本: ${err.response.statusText}\\n`;\n          this.debugInfo += `错误响应数据: ${JSON.stringify(err.response.data || {}, null, 2)}\\n`;\n        } else if (err.request) {\n          this.debugInfo += '请求已发送但没有收到响应\\n';\n          this.debugInfo += `请求URL: ${err.request.responseURL || '未知'}\\n`;\n        } else {\n          this.debugInfo += `请求配置出错: ${err.message || '未知错误'}\\n`;\n          this.debugInfo += `错误详细信息: ${JSON.stringify(err, null, 2)}\\n`;\n        }\n        // 保留测试数据，不清除\n      } finally {\n        this.loading = false;\n        this.debugInfo += '订单数据获取完成\\n';\n      }\n    },\n    showOrderDetail(order) {\n      this.selectedOrder = order;\n      this.showDetail = true;\n    },\n    closeDetail() {\n      this.showDetail = false;\n      this.selectedOrder = null;\n    },\n    formatDate(dateString) {\n      if (!dateString) return '';\n      const date = new Date(dateString);\n      return date.toLocaleDateString('zh-CN');\n    },\n    formatDateTime(dateString) {\n      if (!dateString) return '';\n      const date = new Date(dateString);\n      return date.toLocaleString('zh-CN');\n    },\n    formatPrice(price) {\n      if (typeof price === 'string') {\n        price = parseFloat(price);\n      }\n      return price.toFixed(2);\n    }\n  }\n};","map":{"version":3,"names":["name","data","orders","id","address","created_at","Date","toISOString","is_completed","buyer_name","notes","phone","product_name","price","status","loading","error","statusFilter","timeFilter","showDetail","selectedOrder","debugInfo","created","console","log","fetchOrders","setTimeout","methods","customerUsername","localStorage","getItem","JSON","stringify","params","undefined","sort","headers","$axios","defaults","baseURL","apiParams","username","response","get","statusText","Array","isArray","length","err","message","request","responseURL","showOrderDetail","order","closeDetail","formatDate","dateString","date","toLocaleDateString","formatDateTime","toLocaleString","formatPrice","parseFloat","toFixed"],"sources":["E:\\Software-Engineering2.0\\Software-Engineering-9bea62c8ef6d7754a2d932a0227f9bc4fff09a3c\\litemall\\simple-shop-frontend\\frontend\\src\\views\\CustomerOrders.vue"],"sourcesContent":["<template>\n      <div class=\"order-list\">\n        <h2>我的订单</h2>\n        \n        <!-- 调试信息显示区域 -->\n        <div class=\"debug-info\" v-if=\"debugInfo\">\n          <h4>调试信息</h4>\n          <pre>{{ debugInfo }}</pre>\n        </div>\n        \n        <!-- 筛选条件 -->\n        <div class=\"filter-section\">\n          <div class=\"filter-item\">\n            <label for=\"status-filter\">订单状态：</label>\n            <select id=\"status-filter\" v-model=\"statusFilter\" @change=\"fetchOrders\">\n              <option value=\"\">全部状态</option>\n              <option value=\"pending\">待处理</option>\n              <option value=\"completed\">已完成</option>\n            </select>\n          </div>\n          <div class=\"filter-item\">\n            <label for=\"time-filter\">时间排序：</label>\n            <select id=\"time-filter\" v-model=\"timeFilter\" @change=\"fetchOrders\">\n              <option value=\"desc\">最新优先</option>\n              <option value=\"asc\">最早优先</option>\n            </select>\n          </div>\n        </div>\n\n        <!-- 加载状态 -->\n        <div v-if=\"loading\" class=\"loading\">加载中...</div>\n\n        <!-- 错误提示 -->\n        <div v-if=\"error\" class=\"alert alert-danger\">{{ error }}</div>\n\n        <!-- 订单列表 -->\n        <div v-if=\"orders.length > 0\" class=\"orders-container\">\n      <div v-for=\"order in orders\" :key=\"order.id\" class=\"order-card card\">\n        <div class=\"order-header\">\n          <span class=\"order-id\">订单号: {{ order.id }}</span>\n          <span class=\"order-time\">{{ formatDate(order.created_at) }}</span>\n        </div>\n        \n        <div class=\"order-content\">\n          <div class=\"product-info\">\n            <span class=\"product-name\">{{ order.product_name }}</span>\n            <span class=\"order-amount\">¥{{ order.price ? formatPrice(order.price) : '0.00' }}</span>\n          </div>\n          \n          <div class=\"order-status\">\n            <span class=\"status-badge\" :class=\"order.is_completed ? 'status-completed' : 'status-pending'\">{{ order.is_completed ? '已完成' : '待处理' }}</span>\n            <button class=\"btn btn-secondary\" @click=\"showOrderDetail(order)\">查看详情</button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 无订单提示 -->\n    <div v-else-if=\"!loading\" class=\"alert alert-info\">暂无订单记录</div>\n\n    <!-- 订单详情弹窗 -->\n    <div v-if=\"showDetail\" class=\"modal-overlay\" @click.self=\"closeDetail\">\n      <div class=\"modal-content card\">\n        <div class=\"modal-header\">\n          <h3>订单详情</h3>\n          <button class=\"close-btn\" @click=\"closeDetail\">×</button>\n        </div>\n        \n        <div class=\"modal-body\" v-if=\"selectedOrder\">\n\n          <div class=\"detail-item\">\n            <label>订单编号：</label>\n            <span>{{ selectedOrder.id }}</span>\n          </div>\n          <div class=\"detail-item\">\n            <label>商品名称：</label>\n            <span>{{ selectedOrder.product_name }}</span>\n          </div>\n          <div class=\"detail-item\">\n            <label>订单金额：</label>\n            <span>¥{{ selectedOrder.price ? formatPrice(selectedOrder.price) : '0.00' }}</span>\n          </div>\n          <div class=\"detail-item\">\n            <label>收货人：</label>\n            <span>{{ selectedOrder.buyer_name }}</span>\n          </div>\n          <div class=\"detail-item\">\n            <label>联系电话：</label>\n            <span>{{ selectedOrder.phone }}</span>\n          </div>\n          <div class=\"detail-item\">\n            <label>收货地址：</label>\n            <span>{{ selectedOrder.address }}</span>\n          </div>\n          <div class=\"detail-item\">\n            <label>订单备注：</label>\n            <span>{{ selectedOrder.notes || '无' }}</span>\n          </div>\n          <div class=\"detail-item\">\n            <label>下单时间：</label>\n            <span>{{ formatDateTime(selectedOrder.created_at) }}</span>\n          </div>\n          <div class=\"detail-item\">\n            <label>订单状态：</label>\n            <span class=\"status-badge\" :class=\"selectedOrder.is_completed ? 'status-completed' : 'status-pending'\">{{ selectedOrder.is_completed ? '已完成' : '待处理' }}</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'CustomerOrders',\n  data() {\n      return {\n        orders: [\n          // 测试数据，当API调用失败时也能看到订单列表的显示效果\n          {\n            id: 1,\n            address: '北京市朝阳区某某街道100号',\n            created_at: new Date().toISOString(),\n            is_completed: true,\n            buyer_name: '张三',\n            notes: '请尽快送达',\n            phone: '13800138000',\n            product_name: '测试商品1',\n            price: 99.99,\n            status: 'completed'\n          },\n          {\n            id: 2,\n            address: '上海市浦东新区某某路200号',\n            created_at: new Date().toISOString(),\n            is_completed: false,\n            buyer_name: '李四',\n            notes: '',\n            phone: '13900139000',\n            product_name: '测试商品2',\n            price: 199.99,\n            status: 'pending'\n          }\n        ],\n        loading: false,\n        error: '',\n        statusFilter: '',\n        timeFilter: 'desc',\n        showDetail: false,\n        selectedOrder: null,\n        debugInfo: '' // 用于显示调试信息\n      }\n    },\n  created() {\n      console.log('CustomerOrders组件已创建，准备获取订单数据')\n      // 立即调用fetchOrders方法\n      this.fetchOrders()\n      \n      // 为了测试，再延迟3秒后再次调用\n      setTimeout(() => {\n        console.log('3秒后再次尝试获取订单数据...')\n        this.fetchOrders()\n      }, 3000)\n    },\n  methods: {\n    async fetchOrders() {\n      this.loading = true\n      this.error = ''\n      this.debugInfo = '开始获取订单数据...\\n'\n      \n      try {\n        // 从localStorage获取当前登录用户名\n        const customerUsername = localStorage.getItem('customerUsername')\n        this.debugInfo += `localStorage内容: ${JSON.stringify(localStorage, null, 2)}\\n`\n        this.debugInfo += `获取的customerUsername: ${customerUsername}\\n`\n        \n        // 验证用户是否已登录\n        if (!customerUsername) {\n          this.error = '请先登录再查看订单'\n          this.debugInfo += '未找到登录用户信息\\n'\n          // 保留测试数据，不清除\n          return\n        }\n        \n        this.debugInfo += `当前登录用户: ${customerUsername}\\n`\n        \n        // 构建请求参数\n        const params = {\n          status: this.statusFilter || undefined,\n          sort: this.timeFilter\n        }\n        \n        // 构建请求头\n        const headers = {\n          'X-Username': customerUsername\n        }\n        \n        this.debugInfo += `请求参数: ${JSON.stringify(params, null, 2)}\\n`\n        this.debugInfo += `请求头: ${JSON.stringify(headers, null, 2)}\\n`\n        this.debugInfo += `axios基础URL: ${this.$axios.defaults.baseURL || '未设置'}\\n`\n        // 修复重复API前缀问题 - 基础URL已经包含/api，所以只需要/orders\n        this.debugInfo += `完整请求URL: ${this.$axios.defaults.baseURL ? this.$axios.defaults.baseURL + '/orders' : '/api/orders'}\\n`\n        this.debugInfo += '开始发送API请求...\\n'\n        \n        // 发送API请求，带上筛选参数和用户信息 - 修复API路径和中文编码问题\n        // 修复重复API前缀：基础URL已经包含/api，所以只需要/orders\n        // 修复中文编码问题：不使用X-Username请求头，改用params传递用户名\n        const apiParams = {...params, username: customerUsername} // 将用户名放在query参数中\n        this.debugInfo += `最终请求参数: ${JSON.stringify(apiParams, null, 2)}\\n`\n        \n        const response = await this.$axios.get(`/orders`, {\n          params: apiParams,\n          // 移除包含中文的请求头\n        })\n        \n        this.debugInfo += 'API请求成功!\\n'\n        this.debugInfo += `响应状态: ${response.status}\\n`\n        this.debugInfo += `响应状态文本: ${response.statusText}\\n`\n        this.debugInfo += `API响应数据: ${JSON.stringify(response.data, null, 2)}\\n`\n        \n        // 只有API调用成功时才更新订单数据\n        if (response.data && Array.isArray(response.data)) {\n          this.orders = response.data\n          this.debugInfo += `已更新订单数据，共 ${response.data.length} 条订单\\n`\n        } else {\n          this.debugInfo += 'API返回的数据格式不正确，使用测试数据\\n'\n          this.debugInfo += `响应数据类型: ${typeof response.data}\\n`\n          this.debugInfo += `是否为数组: ${Array.isArray(response.data)}\\n`\n        }\n        \n      } catch (err) {\n        this.error = '获取订单列表失败，显示测试数据'\n        this.debugInfo += `API请求错误: ${err.message || '未知错误'}\\n`\n        if (err.response) {\n          this.debugInfo += `错误响应状态: ${err.response.status}\\n`\n          this.debugInfo += `错误响应状态文本: ${err.response.statusText}\\n`\n          this.debugInfo += `错误响应数据: ${JSON.stringify(err.response.data || {}, null, 2)}\\n`\n        } else if (err.request) {\n          this.debugInfo += '请求已发送但没有收到响应\\n'\n          this.debugInfo += `请求URL: ${err.request.responseURL || '未知'}\\n`\n        } else {\n          this.debugInfo += `请求配置出错: ${err.message || '未知错误'}\\n`\n          this.debugInfo += `错误详细信息: ${JSON.stringify(err, null, 2)}\\n`\n        }\n        // 保留测试数据，不清除\n      } finally {\n        this.loading = false\n        this.debugInfo += '订单数据获取完成\\n'\n      }\n    },\n    \n    showOrderDetail(order) {\n      this.selectedOrder = order\n      this.showDetail = true\n    },\n    \n    closeDetail() {\n      this.showDetail = false\n      this.selectedOrder = null\n    },\n    \n    formatDate(dateString) {\n      if (!dateString) return ''\n      const date = new Date(dateString)\n      return date.toLocaleDateString('zh-CN')\n    },\n    \n    formatDateTime(dateString) {\n      if (!dateString) return ''\n      const date = new Date(dateString)\n      return date.toLocaleString('zh-CN')\n    },\n    \n    formatPrice(price) {\n      if (typeof price === 'string') {\n        price = parseFloat(price)\n      }\n      return price.toFixed(2)\n 
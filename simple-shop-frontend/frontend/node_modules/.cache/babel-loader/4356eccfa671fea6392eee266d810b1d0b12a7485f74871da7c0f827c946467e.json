{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: 'StockLogs',\n  data() {\n    return {\n      stockLogs: [],\n      searchProductName: ''\n    };\n  },\n  mounted() {\n    this.fetchStockLogs();\n  },\n  computed: {\n    // 根据搜索条件过滤日志\n    filteredLogs() {\n      if (!this.searchProductName) {\n        return this.stockLogs;\n      }\n      return this.stockLogs.filter(log => log.product.name.toLowerCase().includes(this.searchProductName.toLowerCase()));\n    }\n  },\n  methods: {\n    async fetchStockLogs() {\n      try {\n        const response = await this.$http.get('/api/stock-logs');\n        this.stockLogs = response.data;\n      } catch (error) {\n        console.error('获取库存日志失败:', error);\n        this.$message.error('获取库存日志失败');\n      }\n    },\n    formatDateTime(dateTimeString) {\n      // 格式化日期时间\n      const date = new Date(dateTimeString);\n      return date.toLocaleString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit'\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","stockLogs","searchProductName","mounted","fetchStockLogs","computed","filteredLogs","filter","log","product","toLowerCase","includes","methods","response","$http","get","error","console","$message","formatDateTime","dateTimeString","date","Date","toLocaleString","year","month","day","hour","minute","second"],"sources":["D:\\github\\最后一舞\\simple-shop-frontend\\frontend\\src\\views\\StockLogs.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2>库存日志</h2>\r\n    <div class=\"stock-logs-container\">\r\n      <div class=\"filter-section\">\r\n        <input\r\n          v-model=\"searchProductName\"\r\n          placeholder=\"搜索商品名称\"\r\n          class=\"search-input\"\r\n        />\r\n        <button class=\"filter-button\">搜索</button>\r\n      </div>\r\n      <div class=\"stock-logs-table-container\">\r\n        <table class=\"stock-logs-table\">\r\n          <thead>\r\n            <tr>\r\n              <th>商品名称</th>\r\n              <th>变更数量</th>\r\n              <th>变更前库存</th>\r\n              <th>变更后库存</th>\r\n              <th>操作类型</th>\r\n              <th>描述</th>\r\n              <th>创建时间</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"log in filteredLogs\" :key=\"log.id\">\r\n              <td>{{ log.product.name }}</td>\r\n              <td :class=\"{ 'positive': log.changeQuantity > 0, 'negative': log.changeQuantity < 0 }\">\r\n                {{ log.changeQuantity > 0 ? '+' : '' }}{{ log.changeQuantity }}\r\n              </td>\r\n              <td>{{ log.previousStock }}</td>\r\n              <td>{{ log.currentStock }}</td>\r\n              <td>{{ log.action }}</td>\r\n              <td>{{ log.description }}</td>\r\n              <td>{{ formatDateTime(log.createdAt) }}</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <div v-if=\"filteredLogs.length === 0\" class=\"empty-message\">\r\n          没有库存日志记录\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'StockLogs',\r\n  data() {\r\n    return {\r\n      stockLogs: [],\r\n      searchProductName: ''\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchStockLogs()\r\n  },\r\n  computed: {\r\n    // 根据搜索条件过滤日志\r\n    filteredLogs() {\r\n      if (!this.searchProductName) {\r\n        return this.stockLogs\r\n      }\r\n      return this.stockLogs.filter(log => \r\n        log.product.name.toLowerCase().includes(this.searchProductName.toLowerCase())\r\n      )\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchStockLogs() {\r\n      try {\r\n        const response = await this.$http.get('/api/stock-logs')\r\n        this.stockLogs = response.data\r\n      } catch (error) {\r\n        console.error('获取库存日志失败:', error)\r\n        this.$message.error('获取库存日志失败')\r\n      }\r\n    },\r\n    formatDateTime(dateTimeString) {\r\n      // 格式化日期时间\r\n      const date = new Date(dateTimeString)\r\n      return date.toLocaleString('zh-CN', {\r\n        year: 'numeric',\r\n        month: '2-digit',\r\n        day: '2-digit',\r\n        hour: '2-digit',\r\n        minute: '2-digit',\r\n        second: '2-digit'\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.stock-logs-container {\r\n  margin-top: 20px;\r\n}\r\n\r\n.filter-section {\r\n  margin-bottom: 20px;\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n\r\n.search-input {\r\n  padding: 8px 12px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  font-size: 14px;\r\n  width: 200px;\r\n}\r\n\r\n.filter-button {\r\n  padding: 8px 16px;\r\n  background-color: #409eff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  font-size: 14px;\r\n}\r\n\r\n.filter-button:hover {\r\n  background-color: #66b1ff;\r\n}\r\n\r\n.stock-logs-table-container {\r\n  overflow-x: auto;\r\n}\r\n\r\n.stock-logs-table {\r\n  width: 100%;\r\n  border-collapse: separate;\r\n  border-spacing: 0;\r\n  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n  border-radius: 4px;\r\n  overflow: hidden;\r\n}\r\n\r\n.stock-logs-table th,\r\n.stock-logs-table td {\r\n  padding: 12px 15px;\r\n  text-align: left;\r\n  border-bottom: 1px solid #eee;\r\n}\r\n\r\n.stock-logs-table th {\r\n  background-color: #f5f7fa;\r\n  font-weight: 700;\r\n  color: #606266;\r\n  white-space: nowrap;\r\n}\r\n\r\n.stock-logs-table tr:hover {\r\n  background-color: #f5f7fa;\r\n}\r\n\r\n.stock-logs-table td.positive {\r\n  color: #67c23a;\r\n  font-weight: 500;\r\n}\r\n\r\n.stock-logs-table td.negative {\r\n  color: #f56c6c;\r\n  font-weight: 500;\r\n}\r\n\r\n.empty-message {\r\n  text-align: center;\r\n  padding: 50px 0;\r\n  color: #909399;\r\n  background-color: white;\r\n  border-radius: 4px;\r\n  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n}\r\n</style>"],"mappings":";;AAgDA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,iBAAiB,EAAE;IACrB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC;EACtB,CAAC;EACDC,QAAQ,EAAE;IACR;IACAC,YAAYA,CAAA,EAAG;MACb,IAAI,CAAC,IAAI,CAACJ,iBAAiB,EAAE;QAC3B,OAAO,IAAI,CAACD,SAAQ;MACtB;MACA,OAAO,IAAI,CAACA,SAAS,CAACM,MAAM,CAACC,GAAE,IAC7BA,GAAG,CAACC,OAAO,CAACV,IAAI,CAACW,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACT,iBAAiB,CAACQ,WAAW,CAAC,CAAC,CAC9E;IACF;EACF,CAAC;EACDE,OAAO,EAAE;IACP,MAAMR,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMS,QAAO,GAAI,MAAM,IAAI,CAACC,KAAK,CAACC,GAAG,CAAC,iBAAiB;QACvD,IAAI,CAACd,SAAQ,GAAIY,QAAQ,CAACb,IAAG;MAC/B,EAAE,OAAOgB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,WAAW,EAAEA,KAAK;QAChC,IAAI,CAACE,QAAQ,CAACF,KAAK,CAAC,UAAU;MAChC;IACF,CAAC;IACDG,cAAcA,CAACC,cAAc,EAAE;MAC7B;MACA,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,cAAc;MACpC,OAAOC,IAAI,CAACE,cAAc,CAAC,OAAO,EAAE;QAClCC,IAAI,EAAE,SAAS;QACfC,KAAK,EAAE,SAAS;QAChBC,GAAG,EAAE,SAAS;QACdC,IAAI,EAAE,SAAS;QACfC,MAAM,EAAE,SAAS;QACjBC,MAAM,EAAE;MACV,CAAC;IACH;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
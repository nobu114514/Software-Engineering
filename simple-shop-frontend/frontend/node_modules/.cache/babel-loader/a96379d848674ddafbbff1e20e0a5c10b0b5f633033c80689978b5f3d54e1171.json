{"ast":null,"code":"import RichTextEditor from '../components/RichTextEditor.vue';\nexport default {\n  name: 'RichTextExample',\n  components: {\n    RichTextEditor\n  },\n  data() {\n    return {\n      editorContent: '<p>欢迎使用富媒体编辑器！</p><p>您可以在这里编辑文本、插入图片等。</p>'\n    };\n  },\n  methods: {\n    saveContent() {\n      // 这里可以添加保存内容的逻辑\n      console.log('保存内容:', this.editorContent);\n      // 例如：调用API保存内容\n      // axios.post('/api/content', { content: this.editorContent })\n      //   .then(response => {\n      //     alert('保存成功！')\n      //   })\n      //   .catch(error => {\n      //     alert('保存失败：' + error.message)\n      //   })\n      alert('内容已保存到控制台！');\n    },\n    clearContent() {\n      // 清空编辑器内容\n      this.editorContent = '';\n    }\n  },\n  mounted() {\n    console.log('富媒体编辑器示例页面已加载');\n  }\n};","map":{"version":3,"names":["RichTextEditor","name","components","data","editorContent","methods","saveContent","console","log","alert","clearContent","mounted"],"sources":["E:\\Software-Engineering\\litemall\\simple-shop-frontend\\frontend\\src\\views\\RichTextExample.vue"],"sourcesContent":["<template>\n  <div class=\"rich-text-example\">\n    <h1>富媒体编辑器示例</h1>\n    \n    <div class=\"editor-section\">\n      <h2>基本用法</h2>\n      <RichTextEditor \n        v-model=\"editorContent\" \n        label=\"编辑内容\"\n      />\n    </div>\n\n    <div class=\"preview-section\">\n      <h2>预览</h2>\n      <div class=\"preview-container\" v-html=\"editorContent\"></div>\n    </div>\n\n    <div class=\"controls\">\n      <button @click=\"saveContent\" class=\"btn-save\">保存内容</button>\n      <button @click=\"clearContent\" class=\"btn-clear\">清空编辑器</button>\n    </div>\n\n    <div class=\"features\">\n      <h2>功能说明</h2>\n      <ul>\n        <li>支持文本格式化（粗体、斜体、对齐方式等）</li>\n        <li>支持图片插入（单次最多10张，大小限制5MB）</li>\n        <li>支持列表、链接、表格等功能</li>\n        <li>支持内容预览</li>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport RichTextEditor from '../components/RichTextEditor.vue'\n\nexport default {\n  name: 'RichTextExample',\n  components: {\n    RichTextEditor\n  },\n  data() {\n    return {\n      editorContent: '<p>欢迎使用富媒体编辑器！</p><p>您可以在这里编辑文本、插入图片等。</p>'\n    }\n  },\n  methods: {\n    saveContent() {\n      // 这里可以添加保存内容的逻辑\n      console.log('保存内容:', this.editorContent)\n      // 例如：调用API保存内容\n      // axios.post('/api/content', { content: this.editorContent })\n      //   .then(response => {\n      //     alert('保存成功！')\n      //   })\n      //   .catch(error => {\n      //     alert('保存失败：' + error.message)\n      //   })\n      alert('内容已保存到控制台！')\n    },\n    clearContent() {\n      // 清空编辑器内容\n      this.editorContent = ''\n    }\n  },\n  mounted() {\n    console.log('富媒体编辑器示例页面已加载')\n  }\n}\n</script>\n\n<style scoped>\n.rich-text-example {\n  max-width: 1000px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\nh1 {\n  color: #333;\n  margin-bottom: 30px;\n  text-align: center;\n}\n\nh2 {\n  color: #555;\n  margin-top: 30px;\n  margin-bottom: 15px;\n  border-bottom: 1px solid #eee;\n  padding-bottom: 8px;\n}\n\n.editor-section,\n.preview-section {\n  background-color: #f9f9f9;\n  padding: 20px;\n  border-radius: 8px;\n  margin-bottom: 20px;\n}\n\n.preview-container {\n  background-color: white;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  padding: 15px;\n  min-height: 200px;\n  max-height: 400px;\n  overflow-y: auto;\n}\n\n.preview-container :deep(p) {\n  margin-bottom: 10px;\n}\n\n.preview-container :deep(img) {\n  max-width: 100%;\n  height: auto;\n}\n\n.controls {\n  margin: 20px 0;\n  display: flex;\n  gap: 10px;\n}\n\n.btn-save,\n.btn-clear {\n  padding: 10px 20px;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 14px;\n  transition: background-color 0.3s;\n}\n\n.btn-save {\n  background-color: #4CAF50;\n  color: white;\n}\n\n.btn-save:hover {\n  background-color: #45a049;\n}\n\n.btn-clear {\n  background-color: #f44336;\n  color: white;\n}\n\n.btn-clear:hover {\n  background-color: #da190b;\n}\n\n.features {\n  background-color: #e8f5e9;\n  padding: 20px;\n  border-radius: 8px;\n  margin-top: 30px;\n}\n\n.features ul {\n  padding-left: 20px;\n}\n\n.features li {\n  margin-bottom: 8px;\n  color: #333;\n}\n</style>"],"mappings":"AAmCA,OAAOA,cAAa,MAAO,kCAAiC;AAE5D,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE;IACjB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ;MACAC,OAAO,CAACC,GAAG,CAAC,OAAO,EAAE,IAAI,CAACJ,aAAa;MACvC;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACAK,KAAK,CAAC,YAAY;IACpB,CAAC;IACDC,YAAYA,CAAA,EAAG;MACb;MACA,IAAI,CAACN,aAAY,GAAI,EAAC;IACxB;EACF,CAAC;EACDO,OAAOA,CAAA,EAAG;IACRJ,OAAO,CAACC,GAAG,CAAC,eAAe;EAC7B;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
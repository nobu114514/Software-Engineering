{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'ChangePassword',\n  data() {\n    return {\n      username: '',\n      oldPassword: '',\n      newPassword: '',\n      error: '',\n      success: false\n    };\n  },\n  methods: {\n    async changePassword() {\n      try {\n        const response = await this.$axios.post('/seller/change-password', null, {\n          params: {\n            username: this.username,\n            oldPassword: this.oldPassword,\n            newPassword: this.newPassword\n          }\n        });\n        if (response.data) {\n          this.success = true;\n          this.error = '';\n          // 重置表单\n          this.username = '';\n          this.oldPassword = '';\n          this.newPassword = '';\n          // 3秒后跳转到后台首页\n          setTimeout(() => {\n            this.$router.push('/seller/dashboard');\n          }, 3000);\n        } else {\n          this.error = '旧密码不正确';\n          this.success = false;\n        }\n      } catch (err) {\n        this.error = '修改密码失败，请重试';\n        this.success = false;\n        console.error(err);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","username","oldPassword","newPassword","error","success","methods","changePassword","response","$axios","post","params","setTimeout","$router","push","err","console"],"sources":["D:\\game\\Trae\\litemall+\\litemall\\simple-shop-frontend\\frontend\\src\\views\\ChangePassword.vue"],"sourcesContent":["<template>\r\n  <div class=\"change-password\">\r\n    <h1>修改密码</h1>\r\n    \r\n    <div v-if=\"error\" class=\"alert alert-danger\">\r\n      {{ error }}\r\n    </div>\r\n    \r\n    <div v-if=\"success\" class=\"alert alert-success\">\r\n      密码修改成功\r\n    </div>\r\n    \r\n    <form @submit.prevent=\"changePassword\" class=\"card\">\r\n      <div class=\"form-group\">\r\n        <label for=\"username\">用户名</label>\r\n        <input type=\"text\" id=\"username\" v-model=\"username\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"oldPassword\">旧密码</label>\r\n        <input type=\"password\" id=\"oldPassword\" v-model=\"oldPassword\" required>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"newPassword\">新密码</label>\r\n        <input type=\"password\" id=\"newPassword\" v-model=\"newPassword\" required>\r\n      </div>\r\n      <div class=\"form-actions\">\r\n        <button type=\"submit\" class=\"btn\">修改密码</button>\r\n        <a href=\"/seller/dashboard\" class=\"btn btn-secondary\">取消</a>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'ChangePassword',\r\n  data() {\r\n    return {\r\n      username: '',\r\n      oldPassword: '',\r\n      newPassword: '',\r\n      error: '',\r\n      success: false\r\n    }\r\n  },\r\n  methods: {\r\n    async changePassword() {\r\n      try {\r\n        const response = await this.$axios.post('/seller/change-password', null, {\r\n          params: {\r\n            username: this.username,\r\n            oldPassword: this.oldPassword,\r\n            newPassword: this.newPassword\r\n          }\r\n        })\r\n        \r\n        if (response.data) {\r\n          this.success = true\r\n          this.error = ''\r\n          // 重置表单\r\n          this.username = ''\r\n          this.oldPassword = ''\r\n          this.newPassword = ''\r\n          // 3秒后跳转到后台首页\r\n          setTimeout(() => {\r\n            this.$router.push('/seller/dashboard')\r\n          }, 3000)\r\n        } else {\r\n          this.error = '旧密码不正确'\r\n          this.success = false\r\n        }\r\n      } catch (err) {\r\n        this.error = '修改密码失败，请重试'\r\n        this.success = false\r\n        console.error(err)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.form-actions {\r\n  margin-top: 1.5rem;\r\n  display: flex;\r\n  gap: 1rem;\r\n}\r\n</style>\r\n"],"mappings":";AAkCA,eAAe;EACbA,IAAI,EAAE,gBAAgB;EACt